{
  "name": "webpack",
  "version": "1.0.0",
  "private": true,
  "description": "React + ES6 + webpack探索",
  "main": "index.js",
  "scripts": {
    "start": "webpack",
    "deploy": "webpack -p --process --colors --profile --config webpack.prod.config.js",
    "dev": "webpack-dev-server --progress --profile --colors --hot --port 9999",
    "test": "jest --verbose",
    "lint": "eslint src"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/GuYunWei/webpack.git"
  },
  "keywords": [
    "webpack",
    "React"
  ],
  "license": "MIT",
  "jest": {
    "scriptPreprocessor": "<rootDir>/node_modules/babel-jest",
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react",
      "<rootDir>/node_modules/react-dom",
      "<rootDir>/node_modules/react-addons-test-utils",
      "<rootDir>/node_modules/fbjs"
    ]
  },
  "devDependencies": {
    "babel-core": "^6.11.4",
    "babel-loader": "^6.2.4",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-plugin-transform-runtime": "^6.12.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.5.0",
    "css-loader": "^0.23.0",
    "expect": "^1.20.2",
    "extract-text-webpack-plugin": "^0.9.1",
    "file-loader": "^0.8.5",
    "html-loader": "^0.4.0",
    "html-webpack-plugin": "^1.7.0",
    "jquery": "^1.11.3",
    "open-browser-webpack-plugin": "0.0.2",
    "react-transform-catch-errors": "^1.0.2",
    "react-transform-hmr": "^1.0.4",
    "redbox-react": "^1.3.0",
    "redux-devtools": "^3.3.1",
    "redux-logger": "^2.7.0",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.9",
    "webpack-dev-server": "^1.14.1"
