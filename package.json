    "fetch-detector": "^1.0.0",
    "fetch-ie8": "^1.4.3",
    "fetch-jsonp": "^1.0.2",
    "history": "^2.1.2",
    "react": "^15.3.1",
    "react-dom": "^15.2.1",
    "react-redux": "^4.4.5",
    "react-router": "^2.6.1",
    "redux": "^3.5.2",
    "redux-actions": "^0.12.0",
    "redux-promise": "^0.5.3",
    "redux-thunk": "^2.1.0",
    "reselect": "^2.5.4"
